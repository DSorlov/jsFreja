{
    "recurseDepth": 1,
    "opts": {
        "encoding": "utf8",
        "recurse": true,
        "readme": "README.md"        
    },
    "plugins": [
        "plugins/markdown",
        "node_modules/docdash-extended/categories",
        "node_modules/jsdoc-export-default-interop/dist/index"
    ],
    "sourceType": "module",
    "tags": {
        "cleverLinks": true,
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "docdash": {
        "static": true,
        "sort": true,
        "search": true,
        "collapse": true,
        "typedefs": true,
        "removeQuotes": "none",
        "private": false,
        "scripts": [],
        "shortenTypes": true,
        "menu":{
            "Github repo": {
                "href":"https://github.com/dsorlov/freja",
                "target":"_blank",
                "class":"menu-item",
                "id":"repository"
            }
        },
        "sectionOrder": [
            "Classes",
            "Categories"
        ]
    },
    "templates": {
        "default": {
            "useLongnameInNav": false,
            "outputSourceFiles": false
        }
    },
    "categoriesFile": "./jsdoc-categories.json"
}